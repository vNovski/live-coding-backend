    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "socket.io": "^4.1.2"
  },
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/socket.io": "^3.0.2",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typecript": "0.0.1-security",
    "typescript": "^4.8.4"
  }
